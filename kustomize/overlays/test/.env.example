# Test Environment Configuration
# Copy this file to .env and fill in actual values

# Database Configuration
DATABASE_HOST=test-database-host
DATABASE_NAME=specify_test
DATABASE_USER=specify
DATABASE_PASSWORD=your-test-password

# Application Configuration
SECRET_KEY=your-test-secret-key
DEBUG=true

# Service URLs (these should work as-is for internal communication)
REDIS_URL=redis://redis-service:6379
ASSET_SERVER_URL=http://asset-server-service:8080
ASSET_SERVER_KEY=your-test-asset-key
REPORT_RUNNER_URL=http://report-runner-service:8088

# Specify6 Configuration
SPECIFY6_DIR=/opt/specify6

# Optional: Additional test-specific settings
LOG_LEVEL=DEBUG
TZ=Australia/Perth